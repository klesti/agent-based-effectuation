<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="maxDepthForMeeting" displayName="Max depth for meeting" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="numberOfCustomers" displayName="Number of Customers" type="int" 
						defaultValue="50" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="minAvailableMoney" displayName="Min available money (thousands EUR)" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="adaptationSpeed" displayName="Adaptation Speed  (1-5)" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="productElementChangeThreshold" displayName="Product element change threshold (%)" type="int" 
						defaultValue="40" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="newConnectionsProbability" displayName="New connections probability" type="double" 
						defaultValue="0.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="utilityFunction" displayName="Utility function" type="java.lang.String" 
						defaultValue="Random" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="Random ConnectionsUtility DegreeCentrality BetweennessCentrality Mixed "
						
		/>
				<parameter name="sampleSizePercentage" displayName="Sample size percentage (Causation)" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="vectorSpaceSize" displayName="Vector Space Size" type="int" 
						defaultValue="5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="numberOfEntrepreneurs" displayName="Number of entrepreneurs" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="marketSplit" displayName="Market Split (%)" type="int" 
						defaultValue="30" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="maxAvailableMoney" displayName="Max available money (thousands EUR)" type="double" 
						defaultValue="10.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="networkGenerator" displayName="Network Generator" type="java.lang.String" 
						defaultValue="BarabasiAlbert" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="BarabasiAlbert CopyModel RandomNetwork SmallWorld "
						
		/>
				<parameter name="edgesPerStep" displayName="Network Generator Edges per step" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="aggregateProductVector" displayName="Aggregate product vector" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									values="true false "
						
		/>
				<parameter name="adaptationThreshold" displayName="Adaptation threshold (%)" type="int" 
						defaultValue="60" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


